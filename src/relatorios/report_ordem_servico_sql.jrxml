	<queryString>
		<![CDATA[SELECT 
    os.id,
    os.data_emissao,
    os.data_entrega,
    os.quantidade,
    pr.pn,
    pr.cliente,
    pr.descricao,
    op.id as op_id,
    op.descricao as op_descricao,
    op.n_operacao as op_n_operacao,
    op.maquina as op_maquina,
    op.tempo_estimado as op_tempo_estimado
FROM
    ordem_servico os
        LEFT OUTER JOIN
    produto pr ON pr.id = os.produto_id
        LEFT OUTER JOIN
    operacao_produto op ON pr.id = op.produto_id
WHERE
    os.id = 38]]>
	</queryString>
